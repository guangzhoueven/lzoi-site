#include <bits/stdc++.h>
using namespace std;
using i64 = long long;

int main() {
	ios::sync_with_stdio(false), cin.tie(nullptr), cout.tie(nullptr);
	i64 n, m;
	cin >> n >> m;
	vector<i64> a(n + 1), dp(n + 1);
	for (i64 i = 1; i <= n; i++) cin >> a[i];

	unordered_map<i64, i64> q;
	q[0] = 0;

	i64 now = 0;
	for (i64 i = 1; i <= n; i++) {
		dp[i] = dp[i - 1];
		now ^= a[i];

		if (q.count(now ^ m)) dp[i] = max(dp[i], dp[q[now ^ m]] + 1);
		q[now] = i;
	}
	cout << dp[n] << "\n";
	return 0;
}
