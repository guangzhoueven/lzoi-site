#include <bits/stdc++.h>
using namespace std;

const int F[] = {6, 2, 5, 5, 4, 5, 6, 3, 7, 6};

int a(long long x) {
	int total = 0;
	while (x > 0) {
		int digit = x % 10;
		total += F[digit];
		x /= 10;
	}
	return total;
}

int solve(long long l, long long r) {
	if (r - l + 1 >= 3) {
		for (long long i = l; i <= l + 100 && i <= r - 2; i++) {
			int s1 = a(i), s2 = a(i + 1), s3 = a(i + 2);
			if (s1 == s2 && s2 == s3) return 3;
		}
	}
	if (r - l + 1 >= 2) {
		for (long long i = l; i <= l + 100 && i <= r - 1; i++) {
			int s1 = a(i), s2 = a(i + 1);
			if (s1 == s2) return 2;
		}
	}
	return 1;
}

int main() {
	ios::sync_with_stdio(false), cin.tie(nullptr), cout.tie(nullptr);
	int T;
	cin >> T;
	while (T--) {
		long long l, r;
		cin >> l >> r;
		cout << solve(l, r) << '\n';
	}
	return 0;
}