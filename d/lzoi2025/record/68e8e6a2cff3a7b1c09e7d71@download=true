#include <bits/stdc++.h>
using namespace std;
using i64 = long long;

int main() {
	ios::sync_with_stdio(false), cin.tie(nullptr), cout.tie(nullptr);
	i64 n;
	cin >> n;

	vector<bool> isPrime(n + 1, true);
	vector<i64> primes;

	for (i64 i = 2; i <= n; i++) {
		if (isPrime[i]) {
			primes.push_back(i);
			for (i64 j = i * i; j <= n; j += i) isPrime[j] = false;
		}
	}

	for (auto p : primes) {
		i64 cnt = 0, power = p;
		while (power <= n) {
			cnt += n / power;
			power *= p;
		}
		cout << p << " " << cnt << "\n";
	}
	return 0;
}