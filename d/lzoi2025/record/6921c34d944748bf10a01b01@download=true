#include <bits/stdc++.h>
using namespace std;
using i64 = long long;

int main() {
	ios::sync_with_stdio(false), cin.tie(nullptr), cout.tie(nullptr);
	i64 n, m;
	cin >> n >> m;
	vector<i64> a(n);
	for (auto& x : a) cin >> x;

	i64 x = 0, y = n - 1, tol = 0;
	vector<i64> cnt(m + 1, 0);

	for (i64 l = 0, r = -1; l < n; l++) {
		while (tol < m && r + 1 < n) {
			if (++cnt[a[++r]] == 1) tol++;
		}
		if (tol == m && r - l < y - x) x = l, y = r;
		if (--cnt[a[l]] == 0) tol--;
	}
	cout << x + 1 << " " << y + 1 << "\n";
	return 0;
}