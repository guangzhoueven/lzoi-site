#include <bits/stdc++.h>
using namespace std;
using i64 = long long;

const i64 MOD = 998244353;
const i64 N = 5e6 + 10;

int main() {
	ios::sync_with_stdio(false), cin.tie(nullptr), cout.tie(nullptr);
	vector<i64> p(N), t(N), s(N), ts(N), pr;
	vector<bool> ip(N, 0);
	
	for (i64 i = 2; i < N; i++) ip[i] = 1;
	p[1] = t[1] = 1;
	for (i64 i = 2; i < N; i++) {
		if (ip[i]) {
			pr.push_back(i);
			p[i] = i - 1;
			t[i] = 2;
		}
		for (i64 x : pr) {
			if (i * x >= N) break;
			ip[i * x] = 0;
			if (i % x == 0) {
				p[i * x] = p[i] * x;
				i64 j = i, c = 0;
				while (j % x == 0) j /= x, c++;
				t[i * x] = t[j] * (c + 2);
				break;
			} else {
				p[i * x] = p[i] * (x - 1);
				t[i * x] = t[i] * 2;
			}
		}
	}
	
	for (i64 i = 1; i < N; i++) {
		ts[i] = (ts[i - 1] + t[i] * t[i]) % MOD;
		s[i] = (s[i - 1] + p[i]) % MOD;
	}
	
	i64 T, n;
	cin >> T;
	while (T--) {
		cin >> n;
		i64 ans = 0;
		for (i64 l = 1, r; l <= n; l = r + 1) {
			r = n / (n / l);
			ans = (ans + (ts[r] - ts[l - 1] + MOD) * s[n / l]) % MOD;
		}
		cout << ans << "\n";
	}
	return 0;
}