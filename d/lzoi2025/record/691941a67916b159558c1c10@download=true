#include <bits/stdc++.h>
using namespace std;
using i64 = long long;

int main() {
	ios::sync_with_stdio(false), cin.tie(nullptr), cout.tie(nullptr);
	i64 n;
	cin >> n;
	vector<i64> a(n);
	for (auto& x : a) cin >> x;

	i64 ma = a[0], mi = a[0], ans = a[0];
	for (i64 i = 1; i < n; i++) {
		i64 x = a[i] * ma, y = a[i] * mi;
		ma = max(a[i], max(x, y));
		mi = min(a[i], min(x, y));  // 考虑负负得正的情况
		ans = max(ans, ma);
	}
	cout << ans << "\n";
	return 0;
}