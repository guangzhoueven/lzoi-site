#include <bits/stdc++.h>
using namespace std;
using i64 = long long;

int main() {
	ios::sync_with_stdio(false), cin.tie(nullptr), cout.tie(nullptr);
	i64 n, k;
	cin >> n >> k;
	
	i64 ans = n * k;
	for (i64 L = 1, R; L <= n; L = R + 1) {// 整除分块[L,R]：L 为当前块起点
		if (k / L == 0) break;  	// k mod i = k
		R = min(n, k / (k / L));  	// 令floor(k/L)=t  =>  L=floot(k/t) => R=floor(k/t) ==> R=floor(k/(k/L))
		// 减去当前块的贡献：sum = t * (L + (L+1) + ... + R) = t * [(L+R)*(R-L+1)/2] = (k/L) * (L+R)*(R-L+1)/2   【等差求和公式】
		ans -= (k / L) * (L + R) * (R - L + 1) / 2;
	}
	cout << ans << "\n";     // k mod i = k - i*floor(k/i)
	return 0;
}