#include <bits/stdc++.h>
using namespace std;
using i64 = long long;

struct Node {
	i64 u;
	i64 v;
	i64 w;
};

i64 kruskal(vector<Node>& g, i64 n, i64 m) {
	vector<i64> p(n + 1);
	sort(g.begin(), g.end(), [&](const auto & x, const auto & y) {
		return x.w < y.w;
	});

//	for (i64 i = 1; i <= n; i++) p[i] = i;
	iota(p.begin(), p.end(), 0);

	function<i64(i64)> find = [&](i64 x) -> i64 {
		if (p[x] != x) p[x] = find(p[x]);
		return p[x];
	};

	i64 ans = 0, cnt = 0;
	for (auto x : g) {
		i64 u = find(x.u), v = find(x.v);
		if (u != v) {
			p[u] = v;
			ans += x.w;
			cnt++;
			if (cnt == n - 1) break;
		}
	}
	if (cnt != n - 1) return -1;
	return ans;
}

int main() {
	ios::sync_with_stdio(false), cin.tie(nullptr), cout.tie(nullptr);
	i64 n, m;
	cin >> n >> m;
	vector<Node> g(m);
	for (i64 i = 0; i < m; i++)
		cin >> g[i].u >> g[i].v >> g[i].w;

	if (kruskal(g, n, m) == -1) cout << "orz\n";
	else cout << kruskal(g, n, m) << "\n";
	return 0;
}