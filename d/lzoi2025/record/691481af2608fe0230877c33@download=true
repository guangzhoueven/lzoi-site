#include <bits/stdc++.h>
using namespace std;
using i64 = long long;

int main() {
	ios::sync_with_stdio(false), cin.tie(nullptr), cout.tie(nullptr);
	i64 L, R;
	cin >> L >> R;
	
	i64 m = sqrt(R);				// 素数范围【2，sqrt(R)】
	vector<i64> p;           		// 素数记录（小范围）
	vector<bool> s(m + 1, true);  	// 素数状态（小范围）
	
	for (i64 i = 2; i <= m; i++)	// 埃筛法
		if (s[i] && (p.push_back(i), true))      // 如果是素数，加入列表
			for (i64 j = i * i; j <= m; j += i)  // 素数的倍数不是素数
				s[j] = false;

	vector<bool> prime(R - L + 1, true);  		// 素数状态（大范围）
	for (auto x : p)  							// 遍历每个小素数
		for (i64 j = max(i64(x) * x, (L + x - 1) / x * x); j <= R; j += x)  // 从x²开始，到>=L的最小x的倍数ceil(L/x)*x 
			prime[j - L] = false;  				// 使用小素数标记大区间 [L, R] 内的合数

	i64 ans = 0;
	for (i64 i = max(2LL, L); i <= R; i++)  	// 从max(2,L)开始，排除1（最小素数2）
		ans += prime[i - L];  					// 如果是素数则计数
	
	cout << ans << "\n";
	return 0;
}