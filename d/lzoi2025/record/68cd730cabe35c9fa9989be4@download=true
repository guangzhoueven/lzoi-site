#include <bits/stdc++.h>
using namespace std;
using i64 = long long;

int main() {
	ios::sync_with_stdio(false), cin.tie(nullptr), cout.tie(nullptr);
	i64 N, V, M;
	cin >> N >> V >> M;
	vector<vector<i64>> dp(V + 1, vector<i64>(M + 1, 0));
	for (i64 i = 0; i < N; i++) {
		i64 v, m, w;
		cin >> v >> m >> w;
		for (i64 j = V; j >= v; j--)
			for (i64 k = M; k >= m; k--)
				dp[j][k] = max(dp[j][k], dp[j - v][k - m] + w);
	}
	cout << dp[V][M] << "\n";
	return 0;
}