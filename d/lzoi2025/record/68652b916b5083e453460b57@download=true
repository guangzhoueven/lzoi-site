#include <bits/stdc++.h>
using namespace std;

bool cmp(int x, int y) {
    bool xEven = (x % 2 == 0);
    bool yEven = (y % 2 == 0);
    if (xEven != yEven) {
        return !xEven; // 如果x是奇数，排在前面
    } else {
        return x < y;   // 如果奇偶性相同，数值小的排在前面
    }
}

int main() {
    int n;
    cin >> n;
    vector<int> a(n);
    for (int i = 0; i < n; ++i) {
        cin >> a[i];
    }
    sort(a.begin(), a.end(), cmp);
    for (int i = 0; i < n; ++i) {
        cout << a[i];
        if (i != n - 1) cout << " ";
    }
    cout << endl;
    return 0;
}
