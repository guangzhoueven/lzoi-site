#include <bits/stdc++.h>
using namespace std;
int main() {
	ios::sync_with_stdio(false);
	cin.tie(0);
	cout.tie(0);
	
	int n;
	cin >> n;
	unordered_map<int, int> freq;
	int max_freq = 0;
	int count_max = 0;
	
	for (int i = 0; i < n; ++i) {
		int x;
		cin >> x;
		freq[x]++;
		int current_freq = freq[x];
		
		if (current_freq > max_freq) {
			max_freq = current_freq;
			count_max = 1;
		} else if (current_freq == max_freq) {
			count_max++;
		}
		
		cout << count_max << ' ' << max_freq << '\n';
	}
	
	return 0;
}