#include <bits/stdc++.h>
using namespace std;
using i64 = long long;
const i64 M = 1e6;

int main() {
	ios::sync_with_stdio(false), cin.tie(nullptr), cout.tie(nullptr);
	i64 n, x, ans = 0;
	cin >> n;
	vector<i64> cnt(M + 1, 0);
	vector<i64> pref(M + 2, 0);

	for (i64 i = 0; i < n; i++) {
		cin >> x;
		cnt[x]++;
	}

	for (i64 i = 1; i <= M; i++) pref[i] = pref[i - 1] + cnt[i];
	for (i64 x = 1; x <= M; x++) {
		if (cnt[x] == 0) continue;	
		ans += cnt[x] * (cnt[x] - 1) / 2;	// 相同数值对
		for (i64 k = 1; k * x <= M; k++) {	// 枚举商 k
			i64 L = k * x;
			i64 R = min((k + 1) * x - 1, M);
			if (L > R) continue;
			
			i64 count = pref[R] - pref[L - 1];			// 统计区间 [L, R] 中 y 的个数（y > x）
			if (L <= x && x <= R) 	count -= cnt[x];  	// 排除 y = x 的情况
			ans += k * cnt[x] * count;
		}
	}
	cout << ans << "\n";
	return 0;
}