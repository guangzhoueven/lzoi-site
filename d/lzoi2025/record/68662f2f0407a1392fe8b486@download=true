#include <iostream>
#include <vector>
#include <map>
#include <string>

using namespace std;

int main() {
	ios::sync_with_stdio(false);
	cin.tie(0);
	cout.tie(0);
	int n, m;
	cin >> n >> m;
	
	map<string, vector<string>> userWebs;
	vector<string> userOrder;
	
	for (int i = 0; i < m; ++i) {
		string name, web;
		cin >> name >> web;
		
		if (userWebs.find(name) == userWebs.end()) {
			userOrder.push_back(name);
		}
		userWebs[name].push_back(web);
	}
	
	for (const string& name : userOrder) {
		cout << name;
		for (const string& web : userWebs[name]) {
			cout << " " << web;
		}
		cout << "\n";
	}
	
	return 0;
}