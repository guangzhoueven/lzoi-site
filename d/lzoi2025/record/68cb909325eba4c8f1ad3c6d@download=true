#include <bits/stdc++.h>
using namespace std;
using i64 = long long;

int main() {
	ios::sync_with_stdio(false), cin.tie(nullptr), cout.tie(nullptr);
	i64 n;
	cin >> n;
	vector<i64> a(n + 1);
	for (i64 i = 1; i <= n; i++) cin >> a[i];

	vector<i64> c = a;
	sort(c.begin() + 1, c.end());
	i64 m = unique(c.begin() + 1, c.end()) - c.begin() - 1;

	vector<i64> b(n + 1);
	for (i64 i = 1; i <= n; i++) b[i] = lower_bound(c.begin() + 1, c.begin() + m + 1, a[i]) - c.begin();

	vector<i64> t(n + 1, 0);
	auto add = [&](i64 i, i64 x) {
		for (; i <= n; i += i & -i) t[i] += x;
	};

	auto ask = [&](i64 i) {
		i64 s = 0;
		for (; i; i -= i & -i) s += t[i];
		return s;
	};

	i64 ans = 0;
	for (i64 i = n; i >= 1; i--) {
		ans += ask(b[i] - 1);
		add(b[i], 1);
	}
	cout << ans << "\n";
	return 0;
}