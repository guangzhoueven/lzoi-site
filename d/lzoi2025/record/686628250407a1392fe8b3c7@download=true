#include <bits/stdc++.h>
using namespace std;

int main() {
	ios::sync_with_stdio(false);
	cin.tie(0);
	cout.tie(0);
	int n, k;
	cin >> n >> k;
	vector<vector<int> > a(n);
	for (int i = 0; i < k; i++) {
		int c;
		cin >> c;
		if (c == 0) {
			int t, x;
			cin >> t >> x;
			a[t].push_back(x);
		} else {
			int t;
			cin >> t;
			if (c == 1) {
				for (vector<int>::iterator it = a[t].begin(); it != a[t].end(); it++) {
					cout << *it << " ";
				}
				cout << "\n";
			}
			if (c == 2) {
				a[t].clear();
			}
		}
	}
	return 0;
}