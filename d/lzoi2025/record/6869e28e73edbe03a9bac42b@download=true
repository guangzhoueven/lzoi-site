#include <iostream>
using namespace std;

int main() {
	ios_base::sync_with_stdio(false);
	cin.tie(0); 	cout.tie(0);

	int T;
	cin >> T;
	while (T--) {
		int n, m, p, q;
		cin >> n >> m >> p >> q;
		
		if (n % p == 0) {
			if (m == (n / p) * q) {
				cout << "YES\n";
			} else {
				cout << "NO\n";
			}
		} else {
			cout << "YES\n";
		}
	}
	return 0;
}