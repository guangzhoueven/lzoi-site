#include <bits/stdc++.h>
using namespace std;
using i64 = long long;

int main() {
	ios::sync_with_stdio(false), cin.tie(0), cout.tie(0);
	i64 T;
	cin >> T;
	while (T--) {
		i64 L, n, m;
		cin >> L >> n >> m;
		vector<i64> p(n);
		for (i64 i = 0; i < n; i++) cin >> p[i];
		sort(p.begin(), p.end());
		
		auto ok = [&](i64 mid) -> bool {
			i64 cnt = 0, x = 0;
			while (x < L) {
				i64 k = x + mid;
				if (k >= L) return true;

				auto it = upper_bound(p.begin(), p.end(), k);
				if (it == p.begin()) return false;
				if (*prev(it) <= x) return false;
				x = *prev(it);
				cnt++;
				if (cnt >= m) return false;
			}
			return true;
		};
		
		i64 left = 1, right = L;
		while (left != right) {
			i64 mid = left + ((right - left) >> 1);
			if (ok(mid)) right = mid;
			else left = mid + 1;
		}
		cout << left << '\n';
	}
	return 0;
}