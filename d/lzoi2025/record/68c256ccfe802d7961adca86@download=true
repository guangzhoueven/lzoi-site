#include <bits/stdc++.h>
using namespace std;
using i64 = long long;

int main() {
	ios::sync_with_stdio(false), cin.tie(0), cout.tie(0);
	i64 n, k;
	cin >> n >> k;
	vector<double> w(n), v(n);
	for (i64 i = 0; i < n; i++) cin >> w[i];
	for (i64 i = 0; i < n; i++) cin >> v[i];
	
	auto ok = [&](double x) -> bool {
		vector<double> a(n);
		for (i64 i = 0; i < n; i++) a[i] = w[i] - x * v[i];
		sort(a.begin(), a.end(), greater<double>());
		
		double sum = 0;
		for (i64 i = 0; i < k; i++) sum += a[i];
		return sum >= 0;
	};
	
	double L = 0, R = 1e5;
	for (i64 i = 0; i < 50; i++) {
		double mid = L + (R - L) / 2;
		if (ok(mid)) L = mid;
		else R = mid;
	}
	cout << fixed << setprecision(2) << L << "\n";
	return 0;
}