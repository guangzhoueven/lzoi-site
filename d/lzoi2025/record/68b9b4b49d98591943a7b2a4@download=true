#include <bits/stdc++.h>
using namespace std;
using i64 = long long;

/* 超市优惠券问题（最小化花费）【贪心算法】*/
int main() {
	ios::sync_with_stdio(false), cin.tie(nullptr), cout.tie(nullptr);
	int T;
	cin >> T;
	while (T--) {
		int n, a, b, x, y;        	// n件商品，a张折扣券，b张立减券，折扣x%，立减y元
		cin >> n >> a >> b >> x >> y;
		vector<double> p(n);       	// n件商品的价格
		for (int i = 0; i < n; i++) cin >> p[i];
		
		// 计算每个商品使用两种优惠后的价格
		vector<double> ZK_price(n), LJ_price(n);
		for (int i = 0; i < n; i++) {
			ZK_price[i] = p[i] * x / 100.0;           // 折扣券后的价格
			LJ_price[i] = max(0.0, p[i] - y);         // 立减券后的价格
		}
		
		// 为每个商品选择最优优惠方式
		vector<pair<double, int>> best_option;        // <最优价格, 商品id>
		for (int i = 0; i < n; i++) {
			best_option.emplace_back(min(ZK_price[i], LJ_price[i]), i);
		}
		sort(best_option.begin(), best_option.end()); // 按最优价格升序排序
		
		double total_cost = 0;
		int ZK_used = 0, LJ_used = 0;
		vector<bool> used(n, false);                  // 标记商品是否已使用优惠券
		
		// 优先选择优惠力度最大的商品
		for (int i = 0; i < n; i++) {
			int idx = best_option[i].second;
			double best_price = best_option[i].first;
			
			if (ZK_price[idx] == best_price && ZK_used < a) {
				total_cost += best_price;
				ZK_used++;
				used[idx] = true;
			} else if (LJ_price[idx] == best_price && LJ_used < b) {
				total_cost += best_price;
				LJ_used++;
				used[idx] = true;
			}
		}
		
		// 处理剩余未使用优惠券的商品
		for (int i = 0; i < n; i++) {
			if (!used[i]) {
				if (ZK_used < a) {
					total_cost += ZK_price[i];
					ZK_used++;
				} else if (LJ_used < b) {
					total_cost += LJ_price[i];
					LJ_used++;
				} else {
					total_cost += p[i];               // 原价购买
				}
			}
		}
		
		cout << fixed << setprecision(4) << total_cost << "\n";
	}
	return 0;
}