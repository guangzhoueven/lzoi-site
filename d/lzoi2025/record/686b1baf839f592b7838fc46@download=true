#include <bits/stdc++.h>
using namespace std;
int main() {
	ios::sync_with_stdio(false);
	cin.tie(0);
	cout.tie(0);
	int n;
	cin >> n;
	int a[n];
	for (int i = 0; i < n; i++) cin >> a[i];
	for (int i = 0; i < n; i++) {
		for (int j = i; j < n; j++) {
			if (a[i] % 2 != 0 && a[j] % 2 != 0 && a[i] > a[j] && a[i] != a[j])swap(a[i], a[j]) ;
		}
	}
	for (int i = 0; i < n; i++) cout << a[i] << " ";
	cout << "\n";
	return 0;
}