#include <bits/stdc++.h>
using namespace std;
using i64 = long long;

int main() {
	ios::sync_with_stdio(false), cin.tie(nullptr), cout.tie(nullptr);
	i64 n, m;
	cin >> n >> m;
	vector<i64> a(n);
	for (auto& x : a) cin >> x;

	i64 x = 0, y = n - 1;				// 最后的答案
	i64 tol = 0;						// 不同数的个数
	unordered_map<i64, i64> cnt(m + 1);	// cnt[i]: i 出现的次数

	for (i64 L = 0, R = -1; L < n; L++) {
		// 当前不同的数还没有达到 m
		while (tol < m && R + 1 < n) {
			// cnt[]++ 之后 cnt[] 为 1，说明 cnt[] 之前为 0，说明这个数是新来的数
			if (++cnt[a[++R]] == 1) tol++;
		}

		// 找到了合法区间，更新答案
		if (tol == m && R - L < y - x) x = L, y = R;

		// 左指针右移，如果 --cnt[a[L]]==0，说明区间内没有这个数了，不同数--
		if (--cnt[a[L]] == 0) tol--;
	}
	cout << x + 1 << " " << y + 1 << "\n";
	return 0;
}