#include <bits/stdc++.h>
using namespace std;

const int dx[4] = {-1, 0, 1, 0};
const int dy[4] = {0, -1, 0, 1};

int n, m;
vector<vector<int>> a;
vector<vector<int>> dp;

int cal(int x, int y) {
    if (dp[x][y] != -1) return dp[x][y];
    dp[x][y] = 1;
    for (int k = 0; k < 4; ++k) {
        int u = x + dx[k], v = y + dy[k];
        if (u >= 0 && u < n && v >= 0 && v < m && a[u][v] < a[x][y]) {
            dp[x][y] = max(dp[x][y], cal(u, v) + 1);
        }
    }
    return dp[x][y];
}

int main() {
    cin >> n >> m;
    a.resize(n, vector<int>(m));
    dp.resize(n, vector<int>(m, -1));
    for (int i = 0; i < n; ++i) {
        for (int j = 0; j < m; ++j) {
            cin >> a[i][j];
        }
    }
    int ans = 0;
    for (int i = 0; i < n; ++i) {
        for (int j = 0; j < m; ++j) {
            ans = max(ans, cal(i, j));
        }
    }
    cout << ans << endl;
    return 0;
}
