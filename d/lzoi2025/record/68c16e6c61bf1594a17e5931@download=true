#include <bits/stdc++.h>
using namespace std;

int main() {
	ios::sync_with_stdio(false), cin.tie(nullptr), cout.tie(nullptr);
	int x, n;
	cin >> x >> n;
	vector<int> a(n);
	for (int i = 0; i < n; i++) cin >> a[i];

	int ans1 = lower_bound(a.begin(), a.end(), x) - a.begin();
	int ans2 = upper_bound(a.begin(), a.end(), x) - a.begin() - 1;

	if (ans1 < n && a[ans1] == x)
		cout << "[" << ans1 + 1 << "," << ans2 + 1 << "]\n";
	else cout << "[-1,-1]\n";
	return 0;
}