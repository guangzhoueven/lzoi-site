#include <bits/stdc++.h>
using namespace std;
using i64 = long long;

int main() {
	ios::sync_with_stdio(false), cin.tie(nullptr), cout.tie(nullptr);
	i64 T;
	cin >> T;
	while (T--) {
		i64 n, ans = 0, tmp = 0;
		cin >> n;
		vector<i64> a(n + 1);
		for (i64 i = 1; i <= n; i++) cin >> a[i];

		unordered_map<i64, i64> q;
		for (i64 L = n; L >= 1; L--) {
			tmp += a[L];
			q[a[L] - tmp]++;
			if (q.count(0 - tmp)) ans = max(ans, q[0 - tmp]);
		}
		cout << ans << "\n";
	}
	return 0;
}