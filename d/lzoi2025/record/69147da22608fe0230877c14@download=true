#include <bits/stdc++.h>
using namespace std;
using i64 = long long;

int main() {
	ios::sync_with_stdio(false), cin.tie(nullptr), cout.tie(nullptr);
	i64 L, R;
	cin >> L >> R;

	i64 m = sqrt(R);
	vector<i64> p;
	vector<bool> s(m + 1, true);

	for (i64 i = 2; i <= m; i++)
		if (s[i] && (p.push_back(i), true))
			for (i64 j = i * i; j <= m; j += i) 
				s[j] = false;

	vector<bool> prime(R - L + 1, true);
	for (auto x : p)
		for (i64 j = max(i64(x) * x, (L + x - 1) / x * x); j <= R; j += x)	
			prime[j - L] = false;

	i64 ans = 0;
	for (i64 i = max(L, 2LL); i <= R; i++)
		ans += prime[i - L];

	cout << ans << "\n";
}