#include <bits/stdc++.h>
using namespace std;
using i64 = long long;

i64 solve(vector<i64> a, i64 n, i64 m) {
	i64 MAXI = 2048, ans = 0;

	vector<i64> L(MAXI + 1), R(MAXI + 1), p(MAXI + 1);
	for (i64 i = 1; i < n; i++) {
		vector<i64> t(MAXI + 1);
		t[a[i]] = 1;
		for (i64 j = 0; j < MAXI; j++) t[j ^ a[i]] += L[j];
		L = t;
	}

	for (i64 i = n; i > 1; i--) {
		vector<i64> t(MAXI + 1);
		t[a[i]] = 1;
		for (i64 j = 0; j < MAXI; j++) t[j ^ a[i]] += R[j];
		R = t;

		for (i64 j = 0; j < MAXI; j++) p[j] += R[j];
		for (i64 j = 0; j < MAXI; j++) ans += L[j] * p[j ^ m];

		L[a[i - 1]]--;
		vector<i64> tmp(MAXI);
		for (i64 j = 0; j < MAXI; j++) tmp[j] = L[j ^ a[i - 1]];
		L = tmp;
	}
	return ans;
}
int main() {
	ios::sync_with_stdio(false), cin.tie(nullptr), cout.tie(nullptr);
	i64 n, m;
	cin >> n >> m;
	vector<i64> a(n);
	for (auto& x : a) cin >> x;
	cout << solve(a, n, m) << "\n";
	return 0;
}