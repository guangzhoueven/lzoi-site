#include <bits/stdc++.h>
using namespace std;
using i64 = long long;

int main() {
	ios::sync_with_stdio(false), cin.tie(nullptr), cout.tie(nullptr);
	i64 T;
	cin >> T;
	while (T--) {
		i64 n, ans = 0;
		cin >> n;
		for (i64 i = 1, j; i <= n; i = j + 1) {
			j = n / (n / i);
			ans += n / i * (j - i + 1);
		}
		cout << ans << "\n";
	}
	return 0;
}