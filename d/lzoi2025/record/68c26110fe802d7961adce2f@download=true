#include <bits/stdc++.h>
using namespace std;
using i64 = long long;

int main() {
	ios::sync_with_stdio(false), cin.tie(0), cout.tie(0);
	i64 n, q, s = 0;
	cin >> n >> q;
	vector<i64> d(n);
	vector<i64> p(n + 1, 0);
	for (i64 i = 0; i < n; i++) cin >> d[i];
	sort(d.begin(), d.end());

	for (i64 i = 1; i <= n; i++) {
		s += d[i - 1];
		p[i] = p[i - 1] + s;
	}

	auto ok = [&](i64 t) -> i64 {
		if (t <= 0) return 0;
		i64 L = 0, R = n;
		while (L != R) {
			i64 mid = L + ((R - L + 1) >> 1);
			if (p[mid] <= t) L = mid;
			else R = mid - 1;
		}
		return L;
	};

	while (q--) {
		i64 t;
		cin >> t;
		cout << ok(t) << "\n";
	}
	return 0;
}