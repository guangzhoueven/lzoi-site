#include <bits/stdc++.h>
using namespace std;
using i64 = long long;

int main() {
	ios::sync_with_stdio(false), cin.tie(0), cout.tie(0);
	i64 n, W;
	cin >> n >> W;
	vector<i64> w(n), t(n);
	for (i64 i = 0; i < n; i++) cin >> w[i] >> t[i];

	auto ok = [&](double mid)->bool {
		vector<double> dp(W + 1, INT_MIN);
		dp[0] = 0;
		for (i64 i = 0; i < n; i++) {
			double s = t[i] - mid * w[i];
			for (i64 j = W; j >= 0; j--) {
				if (dp[j] == INT_MIN) continue;
				i64 nw = min(W, j + w[i]);
				dp[nw] = max(dp[nw], dp[j] + s);
			}
		}
		return dp[W] >= 0;
	};

	double L = 0, R = 1e6;
	for (i64 i = 0; i < 250; i++) {
		double mid = (L + R) / 2;
		(ok(mid) ? L : R) = mid;
	}
	cout << i64(1000 * L + 1e-9) << "\n";
	return 0;
}