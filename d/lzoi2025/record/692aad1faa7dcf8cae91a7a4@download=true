#include <bits/stdc++.h>
using namespace std;
using i64 = long long;

int main() {
	ios::sync_with_stdio(false), cin.tie(nullptr), cout.tie(nullptr);
	i64 n, k;
	cin >> n >> k;

	i64 ans = n * k;
	for (i64 i = 1, j; i <= n; i = j + 1) {
		if (k / i == 0) break;
		j = min(k / (k / i), n);
		ans -= (k / i) * (i + j) * (j - i + 1) / 2;
	}
	cout << ans << "\n";
	return 0;
}