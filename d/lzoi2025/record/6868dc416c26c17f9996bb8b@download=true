#include <bits/stdc++.h>
using namespace std;
inline int first_Digit(int x) {
	if (x < 10) return x;
	int log10 = 1;
	while (x >= 100) {
		x /= 10;
		log10 *= 10;
	}
	return x / 10;
}
inline int last_Digit(int x) {
	return x % 10;
}
int main() {
	ios::sync_with_stdio(false);
	cin.tie(0);
	cout.tie(0);
	int n;
	cin >> n;
	vector<vector<int>> count(10, vector<int>(10, 0));
	for (int i = 1; i <= n; ++i) {
		int a = first_Digit(i);
		int b = last_Digit(i);
		count[a][b]++;
	}
	long long ans = 0;
	for (int a = 1; a <= 9; ++a) {
		for (int b = 1; b <= 9; ++b) {
			ans += (long long)count[a][b] * count[b][a];
		}
	}

	cout << ans << endl;
	return 0;
}