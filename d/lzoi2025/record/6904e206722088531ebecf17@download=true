#include <bits/stdc++.h>
using namespace std;
using i64 = long long;

int main() {
	ios::sync_with_stdio(false), cin.tie(nullptr), cout.tie(nullptr);
	i64 n;
	cin >> n;
	vector<i64> a(n);
	for (i64 i = 0; i < n; i++) cin >> a[i];

	vector<i64> sum(1 << n);
	for (i64 k = 1; k < (1 << n); k++) {
		i64 t = k & -k; // 获取最低位的1
		sum[k] = sum[k ^ t] + a[__builtin_ctzll(t)];// __builtin_ctzll计算整数的二进制中末尾0的个数
	}

	i64 cnt = 0;
	for (i64 k = 1; k < (1 << n); k++) {
		i64 size = __builtin_popcountll(k);
		if (size < 2 || sum[k] % 2 != 0) continue;
		i64 tmp = sum[k] / 2;
		bool ok = false;
		for (i64 j = k & (k - 1); j && !ok; j = (j - 1) & k) {
			if (sum[j] == tmp) {
				cnt++;
				ok = true;
			}
		}
	}
	cout << cnt << "\n";
	return 0;
}