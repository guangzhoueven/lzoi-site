#include <bits/stdc++.h>
using namespace std;
int main() {
	ios::sync_with_stdio(false);
	cin.tie(0);
	cout.tie(0);

	string s;
	cin >> s;
	s = "+" + s; // 统一处理第一个数字
	long long n = s.size(), ans = 0;
	
	for (int i = 0; i < n; ) {
		char op = s[i++]; // 当前运算符
		long long num = 0;
		while (i < n && isdigit(s[i])) { // 检查十进制数字
			num = num * 10 + (s[i] - '0'); // 字符转数字
			i++;
		}
		if (op == '+') {
			ans += num;
		} else {
			ans -= num;
		}
	}
	cout << ans << endl;
	return 0;
}