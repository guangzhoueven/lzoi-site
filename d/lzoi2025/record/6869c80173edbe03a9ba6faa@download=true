#include <bits/stdc++.h>
using namespace std;
int main() {
	ios::sync_with_stdio(false);
	cin.tie(0);
	cout.tie(0);
	int T;
	cin >> T;
	while (T--) {
		int l, r, x, maxn = 0;
		cin >> l >> r >> x;
		for (int i = l; i <= r; i++) {
			maxn = max(maxn, i / x + i % x);
		}
		cout << maxn << "\n";
	}
	return 0;
}
