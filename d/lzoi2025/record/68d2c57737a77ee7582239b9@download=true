#include <bits/stdc++.h>
using namespace std;
using i64 = long long;

vector<bool> ok(vector<i64>& a, i64 k) {
	i64 n = a.size();
	sort(a.begin(), a.end());
	vector<bool> ans(n, false), dp(k + 1, false);
	dp[0] = true;

	for (i64 x = 1, p = -1; x <= n; x++) {
		while (p + 1 < n && a[p + 1] < x) {
			i64 t = a[++p];
			for (i64 j = k; j >= t; j--) {
				if (dp[j - t]) dp[j] = true;
			}
		}

		for (i64 cnt = 0; cnt <= n - p - 1; cnt++) {
			i64 r = k - x * cnt;
			if (r >= 0 && dp[r]) {
				ans[x - 1] = true;
				break;
			}
		}
	}
	return ans;
}

int main() {
	ios::sync_with_stdio(false), cin.tie(nullptr), cout.tie(nullptr);
	int T;
	cin >> T;
	while (T--) {
		i64 n, k;
		cin >> n >> k;
		vector<i64> a(n);
		for (i64 i = 0; i < n; i++) cin >> a[i];
		vector<bool> result = ok(a, k);
		for (i64 i = 0; i < n; i++) cout << result[i] << " \n"[i == n - 1];
	}
	return 0;
}