#include <bits/stdc++.h>
using namespace std;
using i64 = long long;

int main() {
	ios::sync_with_stdio(false), cin.tie(nullptr), cout.tie(nullptr);
	i64 T;
	cin >> T;
	while (T--) {
		i64 n, k;
		cin >> n >> k;
		vector<i64> a(n);
		for (i64 i = 0; i < n; i++) cin >> a[i];

		vector<i64> ans(n + 1, 0);
		for (i64 x = 1; x <= n; x++) {
			bitset<4001> dp;
			dp[0] = 1;
			i64 cnt_x = 0;

			for (i64 num : a) {
				if (num < x) {
					if (num <= 4000) dp |= (dp << num);
				} else cnt_x++;
			}

			for (i64 t = 0; t <= cnt_x; t++) {
				i64 r = k - t * x;
				if (r < 0) break;
				if (r <= 4000 && dp[r]) {
					ans[x] = 1;
					break;
				}
			}
		}
		for (i64 x = 1; x <= n; x++) cout << ans[x] << " \n"[x == n];
	}
	return 0;
}