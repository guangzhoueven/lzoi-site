#include <bits/stdc++.h>
using namespace std;
const int N = 1005;
const int M = 3005;

int n, m;
int v[N], w[N];
int dp[N][M];  // dp[i][j]: 前 i 个物品, 体积不超过 j 的最大价值

// 前 i 个物品, 体积不超过 j 的最大价值
int solve(int i, int j) {
    // 终止条件
    if (i == 0 || j == 0) {
        return 0;
    }
    if (dp[i][j] >= 0) {
        return dp[i][j];
    }
    
    // 分解为规模更小的问题
    // 第一种情况：没用第 i 个物品
    dp[i][j] = solve(i - 1, j);
    
    // 第二种情况：用了第 i 个物品
    if (j >= v[i]) {
        int temp = solve(i - 1, j - v[i]) + w[i];
        if (temp > dp[i][j]) {
            dp[i][j] = temp;
        }
    }
    
    return dp[i][j];
}

int main() {
    cin >> n >> m;
    for (int i = 1; i <= n; i++) {
        cin >> v[i] >> w[i];
    }
    
    // 初始化 dp 数组为 -1
    memset(dp, -1, sizeof(dp));
    
    cout << solve(n, m) << endl;
    return 0;
}
