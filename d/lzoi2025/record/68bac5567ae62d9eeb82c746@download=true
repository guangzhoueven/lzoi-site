#include <bits/stdc++.h>
using namespace std;
int main() {
	ios::sync_with_stdio(false), cin.tie(nullptr), cout.tie(nullptr);
	int T;
	cin >> T;
	while (T--) {
		int n;
		cin >> n;
		vector<int> a(n);
		vector<int> b(n);
		for (int i = 0; i < n; i++) cin >> a[i];
		for (int i = 0; i < n; i++) cin >> b[i];
		if (n == 1) {
			cout << "Yes\n";
			continue;
		} else if (abs(a[0] - b[0]) % 2 == 0) {
			cout << "Yes\n";
			continue;
		} else {
			cout << "No\n";
			continue;
		}
	}
	return 0;
}