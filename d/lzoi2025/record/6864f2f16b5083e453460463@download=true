#include <stdio.h>

#define MOD 998244353

long long power(long long x, long long y) {
    if (y == 0) {
        return 1;
    }
    long long half = power(x, y / 2);
    long long result = (half * half) % MOD;
    if (y % 2 == 1) {
        result = (result * x) % MOD;
    }
    return result;
}

int main() {
    long long x, y;
    scanf("%lld %lld", &x, &y);
    printf("%lld\n", power(x, y));
    return 0;
}