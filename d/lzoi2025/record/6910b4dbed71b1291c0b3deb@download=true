#include <bits/stdc++.h>
using namespace std;
using i64 = long long;

int main() {
	ios::sync_with_stdio(false), cin.tie(nullptr), cout.tie(nullptr);
	i64 n, m, ans = 0;
	cin >> n >> m;
	vector<string> g(n);
	for (auto& r : g) cin >> r;

	vector<i64> h(m + 2, 0);
	for (i64 i = 0; i < n; i++) {
		for (i64 j = 0; j < m; j++)
			h[j + 1] = (g[i][j] == '1') ? h[j + 1] + 1 : 0;

		vector<i64> st = {0};
		for (i64 j = 1; j <= m + 1; j++) {
			while (h[st.back()] > h[j]) {
				i64 H = h[st.back()];
				st.pop_back();
				i64 W = j - st.back() - 1;
				ans = max(ans, H * W);
			}
			st.push_back(j);
		}
	}
	cout << ans << "\n";
	return 0;
}