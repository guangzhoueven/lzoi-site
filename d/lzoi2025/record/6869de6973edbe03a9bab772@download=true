#include <bits/stdc++.h>
using namespace std;
int x[10000020];
int y[10000020];
int main() {
	ios::sync_with_stdio(false);
	cin.tie(0);
	cout.tie(0);
	int n, m;
	cin >> n >> m;
	for (int i = 1; i < n; i++) {
		cin >> x[i];
	}
	int maxn = INT_MIN;
	for (int i = 1; i < n; i++) {
		if (x[i] > maxn) {
			maxn = x[i];
		}
	}
	int min = 1e9;
	for (int i = 1; i < n; i++) {
		if (x[i] < min) {
			min = x[i];
		}
	}
	int ans = 0;
	long long num = 0;
	for (int i = 1; i < n; i++) {
		if (x[i] != maxn and x[i] != min) {
			num = num + x[i];
		}
	}
	if (m - num <= 100) {
		cout << m - num << "\n";
	} else if (m == num) {
		cout << 0 << "\n";
	} else if (m - num > 100) {
		cout << -1 << "\n";
	}
	return 0;
}