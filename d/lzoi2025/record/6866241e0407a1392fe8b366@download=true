#include <bits/stdc++.h>
using namespace std;

bool isPrime(long long num) {  //判断素数 
	if (num <= 1) return false;
	if (num == 2) return true;
	if (num % 2 == 0) return false;
	for (long long i = 3; i <= sqrt(num); i += 2) {
		if (num % i == 0) return false;
	}
	return true;
}

int main() {
	string s;
	cin >> s;
	set<long long> primes;  //素数有序集合：set为自动排序（默认升序、元素唯一，去重） 
	
	for (int i = 0; i < s.length(); i++) {
		for (int j = i; j < s.length(); j++) {
			string sub = s.substr(i, j - i + 1);
			long long num = stoll(sub);  // 字符串转long long 
			if (isPrime(num)) {
				primes.insert(num);      // 若为素数，则插入有序的素数集合中（自动升序排序） 
			}
		}
	}
	
	vector<long long> primesVec(primes.rbegin(), primes.rend());  //反向 
	long long sum = 0;
	int count = min(3, (int)primesVec.size());
	for (int i = 0; i < count; i++) {
		sum += primesVec[i];
	}
	
	cout << sum << "\n";
	return 0;
}