#include <bits/stdc++.h>
using namespace std;
int main() {
	ios::sync_with_stdio(false), cin.tie(nullptr), cout.tie(nullptr);
	int n, m, x, y;
	cin >> n >> m >> x >> y;  		// 马的初始位置在（x,y)上
	x--,y--; 						// 0-base
	
	vector<vector<int>> board(n, vector<int>(m, -1)); // n行m列棋盘board，初始化-1
	queue<pair<int, int>> q;
	q.push({x, y}),	board[x][y] = 0;// 初始化起始点
	
	while (!q.empty()) {
		auto [x, y] = q.front();    // 探索（x,y)点
		q.pop();
		
		// 检查马的八个走位（走“日”）
		vector<int> dx = {2, 1, -1, -2, -2, -1, 1, 2};
		vector<int> dy = {1, 2, 2, 1, -1, -2, -2, -1};
		for (int k = 0; k < 8; k++) {
			int nx = x + dx[k], ny = y + dy[k];	// 更新坐标
			if (nx >= 0 && nx <= n-1 && ny >= 0 && ny <= m-1 && board[nx][ny] == -1) {
				board[nx][ny] = board[x][y] + 1;
				q.push({nx, ny});
			}
		}
	}
	for (int i = 0; i < n; i++) {
		for (int j = 0; j < m; j++) {
			cout << board[i][j] << " ";
		}
		cout << "\n";
	}
	return 0;
}
