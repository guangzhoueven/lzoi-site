#include <bits/stdc++.h>
using namespace std;
typedef long long ll;
int main() {
	ios::sync_with_stdio(false);
	cin.tie(0);
	cout.tie(0);
	ll T;
	cin >> T;
	while (T--) {
		ll n, k, ans = 0;
		cin >> n >> k;
		for (ll i = 1; i <= n; i++) {
			if (n % i == k) ans++;
		}
		cout << ans << "\n";
	}
	return 0;
}