#include <bits/stdc++.h>
using namespace std;
int main() {
	ios::sync_with_stdio(false), cin.tie(nullptr), cout.tie(nullptr);
	double n;
	cin >> n;
	double l = -2000, r = 2000;
	for (int i = 0; i < 60; i++) {
		double mid = (l + r) / 2;
		if (mid * mid * mid < n) l = mid;
		else r = mid;
	}
	cout << fixed << setprecision(6) << l << "\n";
	return 0;
}