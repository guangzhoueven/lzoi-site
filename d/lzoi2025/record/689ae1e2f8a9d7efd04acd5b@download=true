#include <bits/stdc++.h>
using namespace std;

template<typename T>
T extgcd(T a, T b, T &x, T &y) {   //扩展欧几里得算法
	if (!b) {
		x = 1, y = 0;
		return a;
	}
	T x0, y0;
	T g = extgcd(b, a % b, x0, y0);
	x = y0, y = x0 - (a / b) * y0;
	return g;
}

using i64 = long long;

int main() {
	ios::sync_with_stdio(false), cin.tie(nullptr),	cout.tie(nullptr);
	i64 T, a, b, c, x, y;
	cin >> T;
	while (T--) {
		cin >> a >> b >> c;
		i64 d = extgcd(a, b, x, y);
		if (c % d) cout << "0 0" << "\n";
		else cout << x * (c / d) << " " << y * (c / d) << "\n";
	}
	return 0;
}