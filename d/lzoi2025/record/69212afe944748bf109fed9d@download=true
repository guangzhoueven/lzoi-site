#include <bits/stdc++.h>
using namespace std;
using i64 = long long;

int main() {
	ios::sync_with_stdio(false), cin.tie(nullptr), cout.tie(nullptr);
	i64 n, k, x;
	cin >> n >> k >> x;
	vector<i64> a(n), ans;
	for (auto& x : a) cin >> x;

	i64 l = 0, r = n - 1;
	while (r - l + 1 > k) {
		if (abs(a[l] - x) > abs(a[r] - x)) l++;
		else r--;
	}
	for (i64 i = l; i <= r; i++) ans.push_back(a[i]);
	for (i64 i = 0; i < k; i++) cout << ans[i] << " \n"[i == k - 1];
	return 0;
}