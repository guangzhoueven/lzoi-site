#include <bits/stdc++.h>
using namespace std;
using i64 = long long;

int main() {
	ios::sync_with_stdio(false), cin.tie(nullptr), cout.tie(nullptr);
	i64 n, k;
	cin >> n >> k;

	auto ok = [&](i64 n, i64 t) {
		if (t == 0) return n;
		i64 res = 0;
		for (i64 i = 1; i <= n;) {
			i64 q = n / i, r = n / q;
			i64 max_i = min(r, (n - t) / q);
			if (max_i >= i) res += max_i - i + 1;
			i = r + 1;
		}
		return res;
	};

	i64 L = 0, R = n >> 1;
	while (L < R) {
		i64 mid = (L + R + 1) >> 1;
		if (ok(n, mid) >= k) L = mid;
		else R = mid - 1;
	}

	i64 gc = 0, gs = 0;
	for (i64 i = 1; i <= n;) {
		i64 q = n / i, r = n / q;
		i64 max_i = min(r, (n - L - 1) / q);
		if (max_i >= i) {
			i64 cnt = max_i - i + 1;
			i64 f = n - q * i, l = n - q * max_i;
			gc += cnt;
			gs += cnt * (f + l) >> 1;
		}
		i = r + 1;
	}
	cout << gs + (k - gc) * L << "\n";
	return 0;
}