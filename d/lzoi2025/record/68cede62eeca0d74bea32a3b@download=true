#include <bits/stdc++.h>
using namespace std;

int main() {
	ios::sync_with_stdio(false), cin.tie(nullptr), cout.tie(nullptr);
	int T;
	cin >> T;
	while (T--) {
		int n, a, b, x, y;
		cin >> n >> a >> b >> x >> y;
		vector<double> p(n);
		for (int i = 0; i < n; i++) cin >> p[i];
		
		sort(p.begin(), p.end(), greater<double>());
		
		double cost1 = 0.0;
		for (int i = 0; i < min(a, n); i++) cost1 += p[i] * x / 100.0;
		for (int i = n - 1; i >= max(0, n - b); i--) cost1 += max(0.0, p[i] - y);
		for (int i = a; i < n - b; i++) cost1 += p[i];
		
		double cost2 = 0.0;
		int ZK_used = 0, LJ_used = 0;
		
		for (int i = 0; i < n; i++) {
			double ZK_price = p[i] * x / 100.0;
			double LJ_price = max(0.0, p[i] - y);
			
			if (ZK_used < a && LJ_used < b) {
				if (ZK_price < LJ_price) {
					cost2 += ZK_price;
					ZK_used++;
				} else {
					cost2 += LJ_price;
					LJ_used++;
				}
			} else if (ZK_used < a) {
				cost2 += ZK_price;
				ZK_used++;
			} else if (LJ_used < b) {
				cost2 += LJ_price;
				LJ_used++;
			} else {
				cost2 += p[i];
			}
		}
		double ans = min(cost1, cost2);
		cout << fixed << setprecision(4) << ans << "\n";
	}	
	return 0;
}