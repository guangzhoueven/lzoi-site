#include <bits/stdc++.h>
using namespace std;
using i64 = long long;

int main() {
	ios::sync_with_stdio(false), cin.tie(nullptr), cout.tie(nullptr);
	i64 n, r;
	cin >> n >> r;
	
	vector<i64> a;						// 当前组合
	a.reserve(r);						// 预分配容量
	vector<bool> vis(n + 1, false);	
	
	auto dfs = [&](auto&& self, i64 st) -> void {
		if ((i64)a.size() == r) {
			for (i64 i = 0; i < r; i++) cout << a[i] << " \n"[i == r - 1];
			return;
		}
		
		for (i64 i = st; i <= n; i++) {// 从st开始枚举，避免重复组合（按字典序）
			if (!vis[i]) {
				a.push_back(i);
				vis[i] = true;
				self(self, i + 1);		
				a.pop_back();
				vis[i] = false;
			}
		}
	};
	
	dfs(dfs, 1);						// 从数字1开始搜索
	return 0;
}