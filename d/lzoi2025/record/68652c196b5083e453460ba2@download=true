#include <bits/stdc++.h>
using namespace std;

int main() {
    int n;
    cin >> n;
    vector<int> a(n);
    for (int i = 0; i < n; ++i) {
        cin >> a[i];
    }
    vector<int> b(n);
    for (int i = 0; i < n; ++i) {
        b[i] = i;
    }
    sort(b.begin(), b.end(), [&](const int& x, const int& y) {
        return a[x] < a[y];
    });
    for (int i = 0; i < n; ++i) {
        cout << b[i] + 1 << " ";
    }
    cout << endl;
    return 0;
}