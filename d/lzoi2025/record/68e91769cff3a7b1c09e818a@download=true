#include <bits/stdc++.h>
using namespace std;
using i64 = long long;

int main() {
	ios::sync_with_stdio(false), cin.tie(nullptr), cout.tie(nullptr);
	i64 n, m, x;
	cin >> n >> m;

	vector<vector<i64>> a(m + 1);
	for (i64 i = 1; i <= n; i++) {
		cin >> x;
		i64 L;
		if (x >= 0) L = 1;
		else {
			L = (0 - x + i - 1) / i;
			if (L < 1) L = 1;
		}

		i64 R = (n - x) / i;
		if (R > m) R = m;
		if (R < L) continue;

		for (i64 j = L; j <= R; j++) {
			int tmp = x + j * i;
			if (tmp >= 0 && tmp <= n) a[j].push_back(tmp);
		}
	}

	vector<bool> f(n + 2, false);
	for (i64 i = 1; i <= m; i++) {
		for (i64 x : a[i]) 	if (x <= n) f[x] = true;
		i64 p = 0;
		while (f[p]) p++;
		cout << p << "\n";
		for (i64 x : a[i]) 	if (x <= n) f[x] = false;
	}
	return 0;
}