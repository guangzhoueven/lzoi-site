#include <bits/stdc++.h>
using namespace std;
using i64 = long long;

int main() {
	ios::sync_with_stdio(false), cin.tie(nullptr), cout.tie(nullptr);
	i64 n, k;
	cin >> n >> k;
	vector<i64> a(n);
	for (auto& x : a) cin >> x;

	if (k == 0) return cout << "Yes\n", 0;
	auto ok = [&](vector<i64> v) {
		unordered_set<i64> s = {0};
		for (i64 x : v) {
			auto t = s;
			for (i64 y : t) s.insert(x + y);
		}
		return s;
	};

	i64 m = n / 2;
	auto L = ok({a.begin(), a.begin() + m});
	for (i64 R : ok({a.begin() + m, a.end()}))
		if (L.count(k - R))
			return cout << "Yes\n", 0;
	cout << "No\n";
}