#include <bits/stdc++.h>
using namespace std;

int main() {
	ios::sync_with_stdio(false);
	cin.tie(0); cout.tie(0);
	
	int n, m, x;
	cin >> n >> m;
	
	// 合法的数据只有 n * log(n) 个
	vector<vector<int>> a(m + 1);
	for (int i = 1; i <= n; i++) {
		cin >> x;
		int l = (x >= 0 ? 1 : (0 - x + i - 1) / i);
		int r = min((x >= n ? 0 : (n - 1 - x) / i), m);
		for (int j = l; j <= r; j++) a[j].push_back(x + j * i);
	}
	
	vector<bool> f(n + 1, false);
	for (int i = 1; i <= m; i++) {
		for (int x : a[i]) f[x] = true;
		
		int p = 0;
		while (f[p]) p++;
		cout << p << '\n';
		
		for (int x : a[i]) f[x] = false;
	}
	return 0;
}