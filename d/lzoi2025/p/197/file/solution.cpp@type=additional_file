#include <bits/stdc++.h>
using namespace std;

using i64 = long long;

int main() {
    ios::sync_with_stdio(false);
    cin.tie(nullptr); cout.tie(nullptr);

    int n, k;
    cin >> n >> k;

    vector<array<int, 3>> p;
    for (int l = 1, r; l <= n; l = r + 1) {
        int v = n / l;
        r = n / v;
        p.push_back({l, r, v});
    }

    // f[mid] >= k
    auto ok = [&](int mid) {
        int cnt = 0;
        for (auto& [l, r, v] : p) {
            // v * i <= mid  =>  i <= mid / v;
            int u = mid / v;

            // [l, r] <= u 的有多少个
            cnt += max(min(r, u) - l + 1, 0);
        }
        return cnt >= k;
    };

    int l = 1, r = n, mid;
    while (l != r) {
        mid = (l + r) >> 1;
        if (ok(mid)) r = mid;
        else l = mid + 1;
    }
    mid = l;

    i64 ans = 0, cnt = 0;
    for (auto& [l, r, v] : p) {
        // v * i <= mid  =>  i <= mid / v;
        int u = mid / v;

        // [l, r] <= u 的有多少个
        int c = min(r, u);
        if (c >= l) {
            cnt += c - l + 1;
            ans += (i64) v * (l + c) * (c - l + 1) / 2;
        }
    }
    ans -= (i64) mid * (cnt - k);
    cout << (i64) n * k - ans << '\n';

    return 0;
}