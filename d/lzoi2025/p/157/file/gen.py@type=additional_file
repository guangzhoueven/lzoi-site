# /root/anaconda3/envs/py311/bin/python3.11

# https://zhuanlan.zhihu.com/p/593957471
# https://github.com/luogu-dev/cyaron/
from cyaron import *
import random

NL = [1,    1,  10000, 100000, 200000, 300000, 400000, 450000, 490000]
NR = [1, 1000, 100000, 200000, 300000, 400000, 500000, 500000, 500000]
T = len(NL) - 1

for s in range(1, 5):
    for t in range(1, 8 + 1):
        io = IO(file_prefix=str(s) + "-", data_id=t)

        maxk = 10

        # subtask 1: n <= 10^3, l = 1, r = n, 10 pt
        if s == 1:
            n = randint(1, 1000)
            l = 1
            r = n
            io.input_writeln(n, l, r)

        # subtask 2: n <= 10^3, 20 pt
        if s == 2:
            n = randint(1, 1000)
            l = randint(1, min(n, 3))
            r = randint(max(l, n - 3), n)
            io.input_writeln(n, l, r)

        # subtask 3: n <= 5 * 10^5, l = 1, r = n, 30 pt
        if s == 3:
            n = randint(1, 500000)
            l = 1
            r = n
            io.input_writeln(n, l, r)

        # subtask 4: n <= 5 * 10^5, 40 pt
        if s == 4:
            n = randint(400000, 500000)
            l = randint(1, 100000)
            r = randint(400000, n)
            io.input_writeln(n, l, r)
        
        io.output_gen("./a.out")

