#include <bits/stdc++.h>
using namespace std;

vector<bool> solve(vector<int>& a, int k) {
    int n = a.size();
    sort(a.begin(), a.end());
    vector<bool> ans(n, false);
    vector<bool> ok(k + 1, false);
    ok[0] = true;
    for (int i = 1, p = -1; i <= n; i++) {
        while (p + 1 < n && a[p + 1] < i) {
            p++;
            for (int j = k; j >= a[p]; j--) {
                if (ok[j - a[p]]) ok[j] = true;
            }
        }
        // 还剩 n - p 个 i
        for (int j = 0; j <= n - p - 1; j++) {
            int t = k - i * j;
            if (t >= 0 && ok[t]) {
                ans[i - 1] = true;
                break;
            }
        }
    }
    return ans;
}

int main() {
    ios::sync_with_stdio(false);
    cin.tie(0); cout.tie(0);

    int T, n, k;
	cin >> T;
	while (T--) {
        cin >> n >> k;
        vector<int> a(n);
        for (int i = 0; i < n; i++) cin >> a[i];
        vector<bool> ok = solve(a, k);
        for (int i = 0; i < n; i++) cout << (ok[i] ? 1 : 0) << " \n"[i == n - 1];
	}
	return 0;
}