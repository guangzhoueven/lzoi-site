#include <bits/stdc++.h>
using namespace std;

using i64 = long long;
const int P = 998244353;

int main() {
    ios::sync_with_stdio(false);
    cin.tie(0); cout.tie(0);

    int n;
    cin >> n;
    vector<int> a(n);
    for (int i = 0; i < n; i++) cin >> a[i];

    int ans = 0;
    for (int i = 0; i < n; i++) for (int j = i; j < n; j++) {
        int t = a[i] ^ a[j];
        ans = ((i64) ans + t) % P;
    }
    cout << ans << '\n';

    return 0;
}