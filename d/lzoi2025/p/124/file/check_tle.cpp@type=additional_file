#include <bits/stdc++.h>
using namespace std;

const int inf = numeric_limits<int>::max() / 2;

int main() {
	ios::sync_with_stdio(false);
	cin.tie(0); cout.tie(0);
	
	int n, q, x, p, l, r, cmd;
	
	cin >> n >> q;
	vector<int> a(n + 1);
	for (int i = 1; i <= n; i++) cin >> a[i];
	
	while (q--) {
		cin >> cmd;
		if (cmd == 1) {
			cin >> p >> x;
			a[p] = x;
		} else {
			cin >> l >> r;
			int ans = 0;
			for (int i = l; i <= r; i++) ans = max(ans, a[i]);
			cout << ans << '\n';
		}
	}
	return 0;
}