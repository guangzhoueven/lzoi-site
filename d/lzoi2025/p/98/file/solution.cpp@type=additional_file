#include <bits/stdc++.h>
using namespace std;

int main() {
	ios::sync_with_stdio(false);
	cin.tie(0); cout.tie(0);
	
	int T, n, k, x;
	cin >> T;
	while (T--) {
		cin >> n >> k;
		map<int, int> a, b;
		for (int i = 0; i < n; i++) {
			cin >> x;
			x %= k;
			x = min(x, k - x);
			a[x]++;
		}
		for (int i = 0; i < n; i++) {
			cin >> x;
			x %= k;
			x = min(x, k - x);
			b[x]++;
		}
		cout << (a == b ? "YES" : "NO") << '\n';
	}
	return 0;
}