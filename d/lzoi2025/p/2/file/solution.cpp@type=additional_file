#include <bits/stdc++.h>
using namespace std;

vector<int> a(50, 0);            // a[n] 用来存储 f(n) 的值
int f(int n) {
	// 完成终止逻辑
    if (a[n] != 0) return a[n];  // a[n] != 0 表示之前计算过 f(n)，直接返回
	if (n <= 2) return a[n] = 1;
	
	// 分解为规模更小的问题
	a[n] = f(n - 2) + f(n - 1);  // 第一次计算 f(n)，将其结果存入 a[n]
	return a[n];
}

int main() {
	f(30);
    int T, n;
    cin >> T;
    while (T--) {
        cin >> n;
        cout << a[n] << '\n';
    }
	return 0;
}
