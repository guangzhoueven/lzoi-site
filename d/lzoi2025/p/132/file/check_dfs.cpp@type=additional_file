#include <bits/stdc++.h>
using namespace std;

const int inf = numeric_limits<int>::max() / 2;

int main() {
	ios::sync_with_stdio(false);
	cin.tie(0); cout.tie(0);
	
	int n, m;
	cin >> n >> m;

	vector<int> v(n + 1), w(n + 1);
	for (int i = 1; i <= n; i++) cin >> v[i] >> w[i];

    int ans = 0;

    auto dfs = [&](auto&& self, int p, int sum_v, int sum_w) -> void {
        if (p == n + 1) {
            ans = max(ans, sum_w);
            return ;
        }
        self(self, p + 1, sum_v, sum_w);
        if (sum_v + v[p] <= m) self(self, p + 1, sum_v + v[p], sum_w + w[p]);
    };

    dfs(dfs, 1, 0, 0);
    cout << ans << '\n';
	return 0;
}