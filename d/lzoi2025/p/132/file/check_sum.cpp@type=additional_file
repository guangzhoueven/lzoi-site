#include <bits/stdc++.h>
using namespace std;

const int inf = numeric_limits<int>::max() / 2;

int main() {
	ios::sync_with_stdio(false);
	cin.tie(0); cout.tie(0);
	
	int n, m, tol = 0;
	cin >> n >> m;

	vector<int> v(n + 1), w(n + 1);
	for (int i = 1; i <= n; i++) {
		cin >> v[i] >> w[i];
		tol += w[i];  // 总价值
	}
    cout << tol << '\n';

	return 0;
}