#include <bits/stdc++.h>
using namespace std;

int main() {
	int n;
	cin >> n;
	vector<int> a(n);
	for (int i = 0; i < n; i++) cin >> a[i];
	sort(a.begin(), a.end(), [&](const int& x, const int& y) {
		int p = x % 2, q = y % 2;
		if (p != q) return p > q;  // 奇数排在前面
		return x < y;
	});
	for (int i = 0; i < n; i++) cout << a[i] << " \n"[i == n - 1];
	return 0;
}