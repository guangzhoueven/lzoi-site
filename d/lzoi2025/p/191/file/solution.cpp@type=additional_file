#include <bits/stdc++.h>
using namespace std;

using i64 = long long;

int main() {
    ios::sync_with_stdio(false);
    cin.tie(0); cout.tie(0);

    i64 T, n, m;
    cin >> T;
    while (T--) {
        cin >> n >> m;

        i64 ans = 0;
        for (i64 l = 1, r; l <= min(n, m); l = r + 1) {
            r = min(n / (n / l), m / (m / l));
            ans += (n / l) * (m / l) * (r - l + 1);
        }
        cout << ans << '\n';
    }
    return 0;
}