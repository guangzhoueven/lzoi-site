#include <bits/stdc++.h>
using namespace std;

int main() {
	ios::sync_with_stdio(false);
	cin.tie(0); cout.tie(0);
	
	int T, n, x;
	cin >> T;
	while (T--) {
		cin >> n;
		vector<int> a(4, 0);
		for (int i = 0; i < n; i++) cin >> x, a[x % 4]++;
		
		auto ok = [&]() -> bool {
			int maxi = *max_element(a.begin(), a.end());
			if (maxi >= 4) return true;
			for (int i = 0; i <= a[0]; i++) for (int j = 0; j <= a[1]; j++) for (int k = 0; k <= a[2]; k++) {
				int p = 4 - i - j - k;
				if (p >= 0 && p <= a[3] && (j * 1 + k * 2 + p * 3) % 4 == 0) {
					return true;
				}
			}
			return false;
		};
		
		cout << (ok() ? "YES" : "NO") << '\n';
	}
	return 0;
}