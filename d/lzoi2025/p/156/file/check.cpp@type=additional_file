#include <bits/stdc++.h>
using namespace std;

using i64 = long long;

// sqrt(n) 计算 phi(n)
int phi(int n) {
	int ans = n;
	
	// i <= n / i; 比 i * i <= n; 更安全
	// 当 n = 2147483647 时, i * i <= n 会超 int
	for (int i = 2; i <= n / i; i++) {
		if (n % i == 0) {
			while (n % i == 0) n /= i;
			ans = ans / i * (i - 1);  // 这里先除后乘更安全
		}
	}
	// 最后 n 可能还剩一个质数
	// 比如 n = 12 时, 最后会剩下 3
	if (n > 1) ans = ans / n * (n - 1);
	return ans;
}

int main() {	
    ios::sync_with_stdio(false);
    cin.tie(0); cout.tie(0);

    int T, n, d;
    cin >> T;
    while (T--) {
        cin >> n >> d;
        if (n % d) cout << 0 << '\n';  // d 不是 n 的约数
        else cout << phi(n / d) << '\n';
    }
    return 0;
}
