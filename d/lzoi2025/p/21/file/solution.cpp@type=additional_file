#include <bits/stdc++.h>
using namespace std;

using i64 = long long;

int main() {
	ios::sync_with_stdio(false);
	cin.tie(nullptr); cout.tie(nullptr);
	
	vector<vector<int>> a(10, vector<int>(10, 0));
	
	int n;
	cin>> n;
	for (int i = 1; i <= n; i++) {
		int x = i, l, r = i % 10;
		while (x != 0) {
			l = x;
			x /= 10;
		}
		a[l][r]++;
	}
	i64 ans = 0;
	for (int i = 1; i < 10; i++) for (int j = 0; j < 10; j++) {
		ans += (i64) a[i][j] * a[j][i];
	}
	cout << ans << '\n';
	return 0;
}