#include <bits/stdc++.h>
using namespace std;

using i64 = long long;
const int P = 998244353;

int main() {	
    ios::sync_with_stdio(false);
    cin.tie(0); cout.tie(0);

    int n;
    cin >> n;
    vector<i64> f(n + 1, 0);
    for (int i = 1; i <= n; i++) cin >> f[i], f[i] += f[i - 1];

    int ans = 0;
    for (int l = 1; l <= n; l++) for (int r = l; r <= n; r++) {
        ans = ((ans + f[r] - f[l - 1]) % P + P) % P;
    }
    cout << ans << '\n';

    return 0;
}
