#include <bits/stdc++.h>
using namespace std;

template<typename T>
T add(T a, T b) {
	return a + b;
}

template<typename T>
struct Node {
	T a;
};

int main() {

	cout << add(1, 2) << '\n';      // 3
	cout << add(1.1, 2.2) << '\n';  // 3.3
	
	string x = "11", y = "22";
	cout << add(x, y) << '\n';      // 1122
	
	//	cout << add(1, 1.2) << '\n';    // 编译错

	Node<int> x;
	x.a = 1;
	
	Node<string> y;
	y.a = "abc";
	return 0;
}